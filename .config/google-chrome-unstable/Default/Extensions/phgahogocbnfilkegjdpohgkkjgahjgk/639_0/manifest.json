{
   "background": {
      "persistent": true,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/icon64.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "vendor/octicons/octicons.css", "vendor/bentonsans/benton.css", "stylesheets/styles.css" ],
      "js": [ "js/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   }, {
      "js": [ "js/cc_content.js" ],
      "matches": [ "https://*.codeclimate.com/*" ]
   } ],
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "A Chrome extension for Code Climate",
   "homepage_url": "https://codeclimate.com",
   "icons": {
      "128": "icons/icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Code Climate",
   "options_page": "options.html",
   "permissions": [ "webRequest", "webRequestBlocking", "webNavigation", "http://*/*", "https://*/*", "cookies", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "639",
   "web_accessible_resources": [ "badges/*", "icons/*", "vendor/octicons/octicons.eot", "vendor/octicons/octicons.svg", "vendor/octicons/octicons.ttf", "vendor/octicons/octicons.woff", "vendor/bentonsans/*", "vendor/snowplow/2.7.0/*" ]
}
