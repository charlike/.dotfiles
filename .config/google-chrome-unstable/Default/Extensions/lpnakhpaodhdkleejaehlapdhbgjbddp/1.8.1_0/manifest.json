{
   "background": {
      "persistent": false,
      "scripts": [ "api.js" ]
   },
   "content_scripts": [ {
      "css": [ "content.css" ],
      "exclude_matches": [ "https://github.com/*/dotfiles*" ],
      "js": [ "api.js", "node_modules/github-injection/index.js", "content.js" ],
      "matches": [ "https://github.com/*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Hide nonessential files from the file browser",
   "homepage_url": "https://github.com/sindresorhus/hide-files-on-github",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "51",
   "name": "Hide Files on GitHub",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "https://github.com/*/*", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8.1"
}
