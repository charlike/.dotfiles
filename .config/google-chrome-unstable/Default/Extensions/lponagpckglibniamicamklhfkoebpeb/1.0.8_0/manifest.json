{
   "background": {
      "peristent": false,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "inject.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Insert GIFs from Giphy.com into GitHub comments",
   "key": "",
   "manifest_version": 2,
   "name": "GifHub",
   "permissions": [ "https://api.giphy.com/v1/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.8",
   "web_accessible_resources": [ "bundle.js", "extension.css" ]
}
