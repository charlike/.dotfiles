{
   "author": "Stefan Buck",
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "style.css" ],
      "js": [ "app.js" ],
      "matches": [ "https://github.com/*", "https://gist.github.com/*" ],
      "run_at": "document_idle"
   } ],
   "description": "",
   "homepage_url": "https://octolinker.github.io/",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "OctoLinker",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "storage", "https://github.com/", "https://gist.github.com/", "https://githublinker.herokuapp.com/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.12.0"
}
