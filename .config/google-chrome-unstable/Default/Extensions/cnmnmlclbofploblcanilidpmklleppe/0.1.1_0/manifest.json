{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "css": [ "content.css", "dark-mode.css" ],
      "js": [ "vendor/mousetrap.js", "vendor/mousetrap-global-bind.js", "content.js" ],
      "matches": [ "https://m.wikipedia.org/*", "https://*.m.wikipedia.org/*" ],
      "run_at": "document_start"
   } ],
   "description": "Enforces the mobile web version of Wikipedia and improves its interface",
   "homepage_url": "https://github.com/ismamz/refined-wikipedia",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "50",
   "name": "Refined Wikipedia",
   "permissions": [ "webRequest", "webRequestBlocking", "storage", "https://wikipedia.org/*", "https://*.wikipedia.org/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.1"
}
